<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dao.UserRoleDao">
    <select id="getRoleIdByLocalAuthId" resultType="com.entity.UserRole">
        select local_auth_id,role_id from tb_user_roles where local_auth_id=#{localAuthId};
    </select>
</mapper>